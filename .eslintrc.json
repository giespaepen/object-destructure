{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-extend-native": "off"
    }
}